        "controlOutlineWidth": 1
      },
      "Select": {
        "controlHeight": 36,
        "controlHeightLG": 56
      },
      "Radio": {
        "motionDurationMid": "0",
        "motionDurationSlow": "0",
        "controlHeight": 36
