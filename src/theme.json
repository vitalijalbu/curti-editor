{
  "theme": "light",
  "token": {
    "colorPrimary": "#0B7542",
    "borderRadius": 6,
    "fontFamily": "Inter, sans-serif"
    },
    "components": {
      "Typography": {
        "marginXS": "0",
        "marginXXS": "0"
      }, 
      "Card": {
